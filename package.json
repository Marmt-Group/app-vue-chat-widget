{
  "name": "vue-chat-widget",
  "version": "0.3.1",
  "private": false,
  "description": "A lean Vue.js chat ui that is backend agnostic",
  "main": "./dist/vue-chat-widget.common.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-chat-widget ./src/index.js",
    "lint": "vue-cli-service lint",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "vuejs chat",
    "vue chat widget",
    "vue chat ui",
    "vue chat socket",
    "vue simple chat"
  ],
  "author": "david-j-davis <davidjamesdavis.djd@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marmt-Group/app-vue-chat-widget"
  },
  "bugs": {
    "url": "https://github.com/Marmt-Group/app-vue-chat-widget/issues"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.2.1",
    "vue": "^2.6.10",
    "vue-icon": "^2.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^4.1.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "eslint-plugin-vue": "^5.2.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
